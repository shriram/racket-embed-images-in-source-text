#lang racket

(require rackunit)

(define 3x3-red-rect #reader "image-reader.rkt" µ#"AwP/AAD//wAA//8AAP//AAD//wAA//8AAP//AAD//wAA//8AAP8")
(define red-rect #reader "image-reader.rkt" µ#"")
(define red-rect/green-circ #reader "image-reader.rkt" µ#"")

; This will produce the Racket "running man" icon
#reader "image-reader.rkt" µ#""

;; You can also embed images in the middle of other data

; This will produce (list 1 2 <small red dot> 4)
#reader "image-reader.rkt" (list 1 2 µ#"AwP/AAD//wAA//8AAP//AAD//wAA//8AAP//AAD//wAA//8AAP8" 4)

; This will produce (list <image of 1 in red> <image of 2 in green> <image of 3 in blue>)
#reader "image-reader.rkt"
(list µ#""
      µ#""
      µ#"")

(module+ test
  (require 2htdp/image)
  (require "test-images.rkt")

  (check-equal? 3x3-red-rect 3x3-rr)
  (check-equal? red-rect rr)
  (check-equal? red-rect/green-circ rr/gc)
)
